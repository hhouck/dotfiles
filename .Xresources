! hard contrast: *background: #1d2021
*background: #282828
! soft contrast: *background: #32302f
*foreground: #ebdbb2
! Black + DarkGrey
*color0:  #282828
*color8:  #928374
! DarkRed + Red
*color1:  #cc241d
*color9:  #fb4934
! DarkGreen + Green
*color2:  #98971a
*color10: #b8bb26
! DarkYellow + Yellow
*color3:  #d79921
*color11: #fabd2f
! DarkBlue + Blue
*color4:  #458588
*color12: #83a598
! DarkMagenta + Magenta
*color5:  #b16286
*color13: #d3869b
! DarkCyan + Cyan
*color6:  #689d6a
*color14: #8ec07c
! LightGrey + White
*color7:  #a89984
*color15: #ebdbb2

URxvt*scrollBar: false
URxvt*visualBell: true

URxvt*font: xft:Hack Nerd Font Mono:size=12:antialias=true

URxvt.perl-ext-common: clipboard,url-select,resize-font
URxvt.url-select.launcher: firefox
URxvt.url-select.underline: true
URxvt.keysym.M-u: 			perl:url-select:select_next
URxvt.keysym.M-Escape:	perl:keyboard-select:activate
URxvt.keysym.M-s:				perl:keyboard-select:search
URxvt.keysym.C-c:   perl:clipboard:copy
URxvt.keysym.C-v:   perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.keysym.C-k-plus:     perl:resize-font:bigger
URxvt.keysym.C-j-minus:    perl:resize-font:smaller
URxvt.keysym.C-M-equal:    perl:resize-font:reset
URxvt.keysym.C-M-question: perl:resize-font:show
